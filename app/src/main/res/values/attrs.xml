<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="HeaderViewPager">
        <attr name="hvp_topOffset" format="dimension"/>
    </declare-styleable>

    <!--WebViewHorizontalPb-->
    <declare-styleable name="hprogress">
        <attr name="progress" format="integer"/>
        <attr name="max" format="integer"/>
        <attr name="progressHeight" format="integer"/>
        <attr name="progressColor" format="color"/>
    </declare-styleable>

    <!--头像裁剪-->
    <declare-styleable name="ClipViewLayout">
        <attr name="clipType" format="enum">
            <enum name="circle" value="1"/>
            <enum name="rectangle" value="2"/>
        </attr>
        <attr name="mHorizontalPadding" format="dimension"/>
        <attr name="clipBorderWidth" format="dimension"/>
    </declare-styleable>
</resources>